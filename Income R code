 inc=read.csv("income.csv")
> library(arules) 
> inc_trans=as (inc,"transactions")
> summary(inc_trans)
>itemFrequencyPlot(inc_trans[, itemFrequency(inc_trans)>0.5], cex.names=1, main="Item Frequency")
> rules_largeincome=apriori(inc_trans, parameter=list(support=0.1, confidence=0.3, target="rules"), appearance =list(rhs=c("Income=large"), default="lhs"))
> inspect(head(sort(rules_largeincome,by="lift"),10)) inc_rules1=apriori(inc_trans, parameter=list(minlen=2, maxlen=2, support=0.5, confidence=0.8, target="rules"))
>itemset_all=apriori(inc_trans, parameter=list(support = 0.5, target="frequent itemsets"))
>inc_trans1=apriori(inc_trans, parameter = list(support=0.75, confidence=0.75, target="rules"))
>  summary(inc_rules1)
> library(arulesViz)
> plot(inc_rules1, method="grouped")
> (fisher= interestMeasure(rules_largeincome, "fishersExactTest", inc_trans))
> summary(rules_largeincome)
> (fisher_matrix=as(round(fisher,2),"matrix"))
> (rules_dataframe=as(rules_largeincome,"data.frame"))
> write.csv(rules_dataframe,"rules_income.csv")
> write.csv(fisher_matrix,"fisher_income.csv")
